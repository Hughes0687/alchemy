<br>
<div class="row">
  <h2><center>Customer Health</center></h2><hr>
</div>

<div class="row">
  <div id="svgg" class="large-12 large-centered columns"></div>
</div>

<%= javascript_tag do %>
  window.product = <%= raw Product.limit(75).order('size desc').to_json %>
<% end %>


<%= form_tag import_products_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>


<p id="notice"><%= notice %></p>

<div class="row">
<h1><center>Client List</center></h1>

<table>
  <thead>
    <tr>
      <th>Account name</th>
      <th>Total fees</th>
      <th>Healthscore</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.name %></td>
        <td><%= product.size %></td>
        <td><%= product.healthscore %></td>
        <td><%= link_to 'Show', product %></td>
        <td><%= link_to 'Edit', edit_product_path(product) %></td>
        <td><%= link_to 'Destroy', product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%#= link_to 'New Product', new_product_path %>

</div>